body {
	background-color: #1CA39D;
	font-family: Arial;
	margin-left: auto;
	margin-right: auto;
	max-width: 1024px;
	min-width: 256px;
	padding-top: 8px;
	padding-bottom: 24px;
	padding-left: 24px;
	padding-right: 24px;
}

h1 {
	color: #000000;
	font-size: 20px;
	font-family: Tahoma;
	font-weight: 500;
	text-shadow: 1px 1px
}

nav ul {
	list-style-type: none;
	background-color: #336E7F;
	border: 3px solid #111111;
	border-radius: 8px;
	font-family: Tahoma;
	font-size: 16px;
	font-weight: bold;
	padding: 16px;
	
}

nav ul li {
	display: inline;
	border-right: 2px solid #111111;
	padding-right: 8px;
	padding-left: 8px;
}

nav ul li:last-child {
	border-right: none;
}

nav ul li a {
	text-decoration: none;
	color: #111111;
}

hyp ul li a {
	color: #111111;
}